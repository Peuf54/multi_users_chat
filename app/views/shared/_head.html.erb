<title><%= Rails.configuration.application_name %></title>
<meta charset="utf-8">
<meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
<%= tag.meta name: 'current_user_name', content: current_user.name if user_signed_in? %>
<%= tag.meta name: 'current_channel_id', content: params[:id] if current_page?(channel_path) %>

<%= csrf_meta_tags %>
<%= csp_meta_tag %>

<script src="https://kit.fontawesome.com/000d9bfcce.js" crossorigin="anonymous"></script>
<%= stylesheet_link_tag "application", media: "all", "data-turbo-track": "reload" %>
<%= javascript_include_tag "application", "data-turbo-track": "reload", defer: true %>
